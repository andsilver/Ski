/db/*.sqlite3
/db/*.sqlite3-journal
doc/features.html

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp

/public/countries
/public/fixed-banners
/public/resorts
/public/test
/public/up
/public/uploads
.sass-cache/

# Ignore generated RDOC
/doc/app/*

# Ignore FlipKey data files
/flip_key/*
!/flip_key/.keep

# Ignore Interhome data files
/interhome/*
!/interhome/.keep

# Ignore Pierre et Vacances data files
/pierreetvacances/*
!/pierreetvacances/.keep

